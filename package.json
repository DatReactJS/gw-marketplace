    "ethers": "^5.5.2",
    "qs": "^6.10.2",
    "rc-checkbox": "^2.3.2",
    "rc-dialog": "^8.6.0",
    "rc-field-form": "^1.22.0",
    "rc-select": "^13.2.0",
    "rc-slider": "^9.7.4",
    "rc-switch": "^3.2.2",
    "rc-table": "^7.20.3",
    "rc-tabs": "^11.10.4",
    "rc-tooltip": "^5.1.1",
    "react-icomoon": "^2.4.2",
    "react-qrcode-logo": "^2.5.0",
    "react-toastify": "^8.1.0",
